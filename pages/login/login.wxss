.boss {
  width: 100%;
  height: 100%;
  position: absolute;
  background-position: center;
  background-size: 100% 100%;
}

view.wx-logo {
  color: #fff;
  padding-top: 60rpx;
  text-align: center;
}

view.wx-logo image {
  width: 90rpx;
  height: 100rpx;
  margin-left: 10rpx;
  float: left;
}

.suibian {
  width: 500rpx;
  height: 100%;
  margin: 0 auto;
}

view.wx-logp-1 {
  height: 100rpx;
  width: 350rpx;
  font-size: 40rpx;
  line-height: 100rpx;
  display: inline-block;
}

view.search-login {
  flex-direction: row;
  align-items: center;
  margin-bottom: 40rpx;
}

view.searchItem-login {
  display: flex;
  width: 80%;
  height: 70rpx;
  flex-direction: row;
  margin: 0 auto;
  border-bottom: 1rpx rgb(236, 233, 233) solid;
}

view.searchItem-login image {
  height: 42rpx;
  width: 42rpx;
  align-self: center;
  margin-left: 10rpx;
  margin-right: 20rpx;
}

view.searchItem-login input {
  flex: 1;
  font-size: 30rpx;
  color: #000;
  width: 100%;
  height: 70rpx;
}

.place-holder {
  color: #fff;
}

.wx-denglu {
  margin-top: 50rpx;
  width: 80%;
  height: 70rpx;
  background: #fff;
  color: #646df1;
  line-height: 70rpx;
  border-radius: 30px;
}
.Checklist{
  width: 70%;
  margin: 0 auto;
  font-size: 25rpx;
}
checkbox .wx-checkbox-input{
   border-radius: 50%;/* 圆角 */
   width: 23rpx; /* 背景的宽 */
   height: 23rpx; /* 背景的高 */
}
.checkbox{
  float: right;
}