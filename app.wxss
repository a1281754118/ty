/* //需求计划周材品名规格选择弹窗 */
.free-dialog__mask {
 position: fixed;
 top: 0;
 left: 0;
 right: 0;
 bottom: 0;
 z-index: 9999998;
 background: rgba(0, 0, 0, 0.7);
 display: none;
}
.free-dialog__container {
 position: fixed;
 left: 0;
 bottom: 0;
 height: 100%;
 overflow-y: scroll;
 overflow-x: hidden;
 width: 100%;
 background: white;
 transform: translateY(150%);
 transition: all 0.4s ease;
 z-index: 9999999;
}
.free-gunlun{
  margin-top: 140rpx;
  height: 100%;
 
 width: 100%;
/* text-align: center; */
 overflow-y: scroll;
 overflow-x: hidden;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.free-dialog--show .free-dialog__container {
 transform: translateY(0);
}
.free-dialog--show .free-dialog__mask {
 display: block;
}
